@model Article





<div class="container">
    <h1>Delete Article</h1>
    <form method="POST">


        <div class="form-group">
            <label for="Title">Title</label>
            <input type="text" disabled class="form-control" id="Title" asp-for="Title">

        </div>
        <div class="form-group">
            <label for="textarea">Description</label>
            <textarea class="form-control" id="textarea" rows="5" asp-for="Description" disabled></textarea>
        </div>
        <button type="submit" class="btn btn-primary">Delete</button>


    </form>

</div>